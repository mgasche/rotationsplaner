      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
        <ul class="navbar-nav">
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="/users" id="dropdown10" data-bs-toggle="dropdown" aria-expanded="false"><svg class="bi me-2" fill="grey" width="40" height="32"><use xlink:href="#user"/></svg>User</a>
            <ul class="dropdown-menu" aria-labelledby="dropdown10">
              <li><a class="dropdown-item" href="/users">Users</a></li>
              <li><a class="dropdown-item" href="/users/new">New User</a></li>
            </ul>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="/locations" id="dropdown10" data-bs-toggle="dropdown" aria-expanded="false"><svg class="bi me-2" fill="grey" width="40" height="32"><use xlink:href="#house"/></svg>Location</a>
            <ul class="dropdown-menu" aria-labelledby="dropdown10">
              <li><a class="dropdown-item" href="/locations">Locations</a></li>
              <li><a class="dropdown-item" href="/locations/new">New Location</a></li>
            </ul>
          </li>
        </ul>
      </div>
    </div>
