      <a href="/users" class="col-md-4 d-flex align-items-center justify-content-center mb-3 mb-md-0 me-md-auto link-dark text-decoration-none">
        <%= image_tag 'baselland-fkd.png', width: 181.25, height: 59.25, alt: 'Baselland FKD'%>
      </a>
      <ul class="nav col-md-4 justify-content-end">
        <li class="nav-item"><a href="/users" class="nav-link px-2 text-muted">User</a></li>
        <li class="nav-item"><a href="/locations" class="nav-link px-2 text-muted">Locations</a></li>
